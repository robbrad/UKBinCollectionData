{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": [
        "BroadlandDistrictCouncil",
        "https://area.southnorfolkandbroadland.gov.uk/FindAddress",
        "-n",
        "1 Park View, Horsford, Norfolk, NR10 3FD",
        "-p",
        "NR10 3FD",
        "-w",
        "http://selenium-hub:4444",
      ],
    },
    {
      "name": "Python Debugger: Integ Tests",
      "type": "debugpy",
      "request": "launch",
      "purpose": [
        "debug-test"
      ],
      "env": {
        "PYTEST_ADDOPTS": "--headless=False --local_browser=True"
      }
    }
  ]
}